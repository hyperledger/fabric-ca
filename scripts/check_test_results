if [ $# -ne 1 ]; then
   echo "Usage: check_test_results <results-file>"
   exit 1
fi

TEST_RESULTS=$1

SC=0
FAILURES=`awk '$1 != "ok" && $1 != "?" && $1$2 != "#testmain" && $1$2$3$4 != "ld:warning:PIEdisabled."' $TEST_RESULTS`
if [ "$FAILURES" != "" ]; then
   echo "*** BEGIN FAILURES ***"
   echo "$FAILURES"
   echo "*** END FAILURES ***"
   SC=1
fi
exit $SC
